{
    "name": "webpacktest",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack-dev-server --open",
        "build": "webpack --config webpack.config.js"
    },
    "license": "MIT",
    "eslintConfig": {
        "root": true,
        "parserOptions": {
            "ecmaVersion": 2017,
            "extends": "standard",
            "parser": "babel-eslint"
        },
        "extends": [
            "mysticatea/modules",
            "plugin:prettier/recommended",
            "plugin:vue/recommended",
            "prettier/vue"
        ],
        "plugins": [
            "node",
            "vue"
        ],
        "env": {
            "browser": false
        },
        "globals": {
            "applicationCache": false,
            "atob": false,
            "btoa": false,
            "console": false,
            "document": false,
            "location": false,
            "window": false
        },
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "tabWidth": 4,
                    "printWidth": 80,
                    "semi": false,
                    "singleQuote": false,
                    "trailingComma": true
                }
            ],
            "node/no-extraneous-import": "error",
            "node/no-missing-import": "error",
            "node/no-unpublished-import": "error",
            "indent": 0,
            "vue/html-indent": [
                "error",
                4
            ],
            "vue/max-attributes-per-line": "off"
        }
    },
    "devDependencies": {
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-stage-2": "^6.22.0",
        "babel-register": "^6.22.0",
        "clean-webpack-plugin": "^2.0.1",
        "connect-history-api-fallback": "^1.3.0",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.0",
        "eslint": "^5.0.0",
        "eslint-config-mysticatea": "^13.0.2",
        "eslint-config-prettier": "^4.1.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-eslint-comments": "^2.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-mysticatea": "^4.2.4",
        "eslint-plugin-node": "^7.0.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-promise": "^4.0.0",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^4.7.1",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^2.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "less": "^2.7.1",
        "less-loader": "^2.2.3",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.11.0",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.1.2",
        "prettier": "^1.16.4",
        "px2rem-loader": "^0.1.9",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "url-loader": "^1.1.2",
        "vue-eslint-parser": "^6.0.3",
        "vue-loader": "^13.3.0",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.5.2",
        "vux-loader": "^1.0.58",
        "webpack": "^3.6.0",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dev-middleware": "^1.10.0",
        "webpack-dev-server": "^2.9.1",
        "webpack-hot-middleware": "^2.16.1",
        "webpack-merge": "^4.1.0",
        "yaml-loader": "^0.4.0"
    },
    "dependencies": {
        "vux": "^2.2.0",
        "fastclick": "^1.0.6",
        "vuex-i18n": "^1.3.1",
        "vuex": "^2.1.1",
        "vue-router": "^3.0.1",
        "vue": "^2.5.2"
    }
}